;; Eval Buffer with `M-x eval-buffer' to register the newly created template.

(dap-register-debug-template
  "Python :: Run python Chana"
  (list :type "python"
        :args ""
        :cwd nil
        :module nil
        :env '(("DEBUG" . "1"))
        :program "/work/python/MoneyPrinterTurbo/main.py"
        :request "launch"
        :name "Python :: Run file (buffer)"))
